{
  "name": "securepass-root",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd my-app && npm run dev",
    "install:all": "npm install && cd server && npm install && cd ../my-app && npm install",
    "build": "cd my-app && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
